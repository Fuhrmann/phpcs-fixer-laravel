{
    "name": "fuhrmann/phpcs-fixer-laravel",
    "type": "project",
    "description": "Rules for PhpCsFixer used in Laravel projects.",
    "keywords": [
        "phpcsfixer",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "friendsofphp/php-cs-fixer": "^2.18",
        "brainmaestro/composer-git-hooks": "^2.8"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "php-cs-fixer": "vendor/bin/php-cs-fixer",
        "fix-style": "php-cs-fixer fix --config=.php_cs.dist --ansi"
    }
}
